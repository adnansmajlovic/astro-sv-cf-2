{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "types": [
      "./worker-configuration.d.ts",
      "node"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "$/*": [
        "src/*"
      ],
      "$api/*": [
        "src/api/*"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$lib/*": [
        "src/lib/*"
      ],
    }
  }
}
